import { handleActions } from 'redux-actions'

import {
  tempSetInfo,
} from './action'

export const namespace = '{{moduleName}}'

export const defaultState = {
  temp: '{{moduleName}}',
}

export const {{moduleName}}Reducer = handleActions(
  {
    // ç¤ºä¾‹
    [tempSetInfo]: (state, action) => {
      const { data } = action.payload
      return { ...state, data }
    },
  },
  defaultState
)

